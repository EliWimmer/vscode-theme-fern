    {
      "name": "Powershell hashtable member",
      "scope": [
        "source.powershell meta.hashtable.assignment.powershell variable.other.readwrite.powershell"
      ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    // SVELTE ----------------------------------------
    {
      "scope": "entity.name.tag.svelte",
      "settings": {
        "foreground": "#798C3B"
      }
    },

    {
      "scope": "entity.other.attribute-name.svelte",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "support.class.component.svelte",
      "settings": {
        "foreground": "#B15D43"
      }
    }
  ],
  "colors": {
    // BASE COLORS
    "focusBorder": "#363C38",
    "foreground": "#202725",
    "widget.shadow": "#191F1D00",
    "selection.background": "#32754B80",
    "errorForeground": "#9F733C",
