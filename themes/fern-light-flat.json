{
  "$schema": "vscode://schemas/color-theme",
  "name": "Fern Dark Medium",
  "type": "dark",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "settings": {
        "foreground": "#202725",
        "fontStyle": "bold",
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#2C7A67",
        "fontStyle": "bold",
      }
    },
    {
      "scope": [ "comment", "punctuation.definition.comment" ],
      "settings": {
        "foreground": "#7C837E",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [ "constant", "support.constant", "variable.arguments" ],
      "settings": {
        "foreground": "#826381",
        "fontStyle": "bold",
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "#202725",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#4F9268",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [ "entity.name.tag", "punctuation.tag" ],
      "settings": {
        "foreground": "#4F9268",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [ "invalid", "invalid.illegal" ],
      "settings": {
        "foreground": "#943F27",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#826381",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#4F9268",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#B15D43",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#4F9268",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.header.diff",
      "settings": {
        "foreground": "#B15D43",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#B15D43",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [ "storage.type", "storage.modifier" ],
      "settings": {
        "foreground": "#B15D43",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#4F9268",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#B15D43",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string.escape",
      "settings": {
        "foreground": "#943F27"
      }
    },
    {
      "scope": "string.quasi",
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "string.entity",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": "object",
      "settings": {
        "foreground": "#202725"
      }
    },
    {
      "scope": "module.node",
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#32754B"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#B15D43"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "keyword.control.module",
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "keyword.control.less",
      "settings": {
        "foreground": "#858701"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": "metatag.php",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#32754B"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.type.name",
        "meta.return.type",
        "meta.return-type",
        "meta.cast",
        "meta.type.annotation",
        "support.type",
        "storage.type.cs",
        "variable.class"
      ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": [ "variable.this", "support.variable" ],
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.static",
        "entity.name.class.static.function",
        "entity.name.function",
        "entity.name.class",
      ],
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "name": "Function declarations",
      "scope": [ "entity.function", "entity.name.function.static", "entity.name.function" ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": [ "entity.name.function.function-call", "meta.function-call.ts" ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "support.function.builtin",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "entity.name.method.function-call",
        "entity.name.static.function-call"
      ],
      "settings": {
        "foreground": "#32754B"
      }
    },
    {
      "scope": "brace",
      "settings": {
        "foreground": "#2D3430"
      }
    },
    {
      "name": "variables",
      "scope": [
        "meta.parameter.type.variable",
        "variable.parameter",
        "variable.name",
        "variable.other",
        "variable",
        "string.constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "scope": "prototype",
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": [ "punctuation" ],
      "settings": {
        "foreground": "#2D3430"
      }
    },
    {
      "scope": "punctuation.quoted",
      "settings": {
        "foreground": "#202725"
      }
    },
    {
      "scope": "punctuation.quasi",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "name": "URL",
      "scope": [ "*url*", "*link*", "*uri*" ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    // ----------------------------------------
    //            LANGUAGE SPECIFIC
    // ----------------------------------------
    // PYTHON ----------------------------------------
    {
      "name": "Python function",
      "scope": [ "meta.function.python", "entity.name.function.python" ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "name": "Python Function and Class definition keywords",
      "scope": [
        "storage.type.function.python",
        "storage.modifier.declaration",
        "storage.type.class.python",
        "storage.type.string.python"
      ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "name": "Async keyword",
      "scope": [ "storage.type.function.async.python" ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "name": "Python Function Call",
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "name": "Python Function Arguments",
      "scope": "meta.function-call.arguments",
      "settings": {
        "foreground": "#2D3430"
      }
    },
    {
      "name": "Python Function decorator",
      "scope": "entity.name.function.decorator",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python ALL CAPS",
      "scope": "constant.other.caps",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // SHELL ----------------------------------------
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "punctuation.definition.logical-expression",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": [
        "string.interpolated.dollar.shell",
        "string.interpolated.backtick.shell"
      ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    // C C++ ----------------------------------------
    {
      "scope": "keyword.control.directive",
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "support.function.C99",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    // C# ----------------------------------------
    {
      "name": "C# functions & namespace",
      "scope": [
        "meta.function.cs",
        "entity.name.function.cs",
        "entity.name.type.namespace.cs"
      ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "name": "C# Variables",
      "scope": [
        "keyword.other.using.cs",
        "entity.name.variable.field.cs",
        "entity.name.variable.local.cs",
        "variable.other.readwrite.cs"
      ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "name": "C# This",
      "scope": [ "keyword.other.this.cs", "keyword.other.base.cs" ],
      "settings": {
        "foreground": "#826381"
      }
    },
    // MAKEFILE ----------------------------------------
    {
      "scope": "meta.scope.prerequisites",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": "entity.name.function.target",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    // JAVA ----------------------------------------
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#252B28"
      }
    },
    {
      "scope": [ "keyword.other.import.java", "keyword.other.package.java" ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "storage.type.java",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": "storage.type.annotation",
      "settings": {
        "foreground": "#499784",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other.documentation.javadoc",
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "scope": "comment.block.javadoc variable.parameter.java",
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "source.java variable.other.object",
        "source.java variable.other.definition.java"
      ],
      "settings": {
        "foreground": "#202725"
      }
    },
    // LISP ----------------------------------------
    {
      "name": "Lisp optional function parameters",
      "scope": "meta.function-parameters.lisp",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    // MARKUP ----------------------------------------
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#252B28",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#82561F"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "markup.punctuation.quote.beginning",
      "settings": {
        "foreground": "#5C6F1E"
      }
    },
    {
      "scope": "markup.punctuation.list.beginning",
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "scope": [ "markup.inline.raw", "markup.fenced_code.block" ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    // JSON ----------------------------------------
    {
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "name": "JSON Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "name": "JSON Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#4F9268"
      }
    },
    {
      "name": "JSON Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "name": "JSON Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    // CSS ----------------------------------------
    {
      "scope": "entity.other.attribute-name.css",
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "source.css meta.selector",
      "settings": {
        "foreground": "#202725"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#202725"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": [
        "source.css support.function.transform",
        "source.css support.function.timing-function",
        "source.css support.function.misc"
      ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.property-value",
        "constant.rgb-value",
        "support.property-value.scss",
        "constant.rgb-value.scss"
      ],
      "settings": {
        "foreground": "#82561F"
      }
    },
    {
      "scope": [ "entity.name.tag.css" ],
      "settings": {
        "fontStyle": ""
      }
    },
    // HTML / XML ----------------------------------------
    {
      "scope": [ "punctuation.definition.tag" ],
      "settings": {
        "foreground": "#499784"
      }
    },
    {
      "scope": [ "text.html entity.name.tag", "text.html punctuation.tag" ],
      "settings": {
        "foreground": "#798C3B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#9F733C",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [ "punctuation.definition.string.end.html", "punctuation.definition.string.begin.html" ],
      "settings": {
        "foreground": "#32754B",
        "fontStyle": "bold"
      }
    },
    // javascript ---------------------------------------
    {
      "scope": [ "source.js variable.language" ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    // typescript ---------------------------------------
    {
      "scope": [ "source.ts variable.language" ],
      "settings": {
        "foreground": "#9F733C"
      }
    },
    {
      "scope": "meta.function-call.ts",
      "settings": {
        "foreground": "#826381"
      }
    },
    {
      "scope": "variable.other.object.ts",
      "settings": {
        "foreground": "#202725"
      }
    },
    {
      "scope": "entity.name.function.ts",
      "settings": {
        "foreground": "#798C3B"
      }
    },
    // JSDoc --------------------------------------------
    {
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#943F27"
      }
    },
    {
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#32754B"
      }
    },
    // golang --------------------------------------------
    {
      "scope": [ "source.go storage.type" ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": [ "source.go entity.name.import" ],
      "settings": {
        "foreground": "#798C3B"
      }
    },
    {
      "scope": [ "source.go keyword.package", "source.go keyword.import" ],
